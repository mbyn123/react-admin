itorChange={this.handleEditorChange}\r\n            />\r\n\r\n\r\n        );\r\n    }\r\n}\r\n\r\nexport default Editors;\r\n",{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","severity":1,"message":"152","line":50,"column":15,"nodeType":"153","messageId":"154","endLine":50,"endColumn":19},{"ruleId":"151","severity":1,"message":"155","line":3,"column":10,"nodeType":"153","messageId":"154","endLine":3,"endColumn":21},{"ruleId":"151","severity":1,"message":"156","line":4,"column":8,"nodeType":"153","messageId":"154","endLine":4,"endColumn":18},{"ruleId":"151","severity":1,"message":"157","line":5,"column":10,"nodeType":"153","messageId":"154","endLine":5,"endColumn":17},{"ruleId":"158","severity":1,"message":"159","line":5,"column":5,"nodeType":"160","messageId":"161","endLine":7,"endColumn":6},"no-native-reassign",["162"],"no-negated-in-lhs",["163"],"no-unused-vars","'name' is assigned a value but never used.","Identifier","unusedVar","'requestData' is defined but never used.","'requestUrl' is defined but never used.","'message' is defined but never used.","no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor","no-global-assign","no-unsafe-negation"]