                 number: 0,\r\n                    status: true\r\n                },\r\n                setFieldValue: {},\r\n                formItem: [\r\n                    {\r\n                        type: 'Input',\r\n                        label: \"部门名称\",\r\n                        name: \"name\",\r\n                        required: true\r\n                    },\r\n                    {\r\n                        type: 'InputNumber',\r\n                        label: \"人员数量\",\r\n                        name: \"number\",\r\n                        min: 0,\r\n                        max: 100,\r\n                        required: true\r\n                    },\r\n                    {\r\n                        type: 'Radio',\r\n                        label: \"禁启用\",\r\n                        name: \"status\",\r\n                        options: [{ label: '禁用', value: false }, { label: '启用', value: true }],\r\n                        required: true\r\n                    },\r\n                    {\r\n                        type: 'TextArea',\r\n                        label: \"描述\",\r\n                        name: \"content\",\r\n                        required: true\r\n                    }\r\n                ]\r\n            },\r\n\r\n        }\r\n\r\n    }\r\n    componentDidMount () {\r\n        if (this.props.location.state) {\r\n            let { id } = this.props.location.state\r\n            this.getDepartmentDetailed(id)\r\n            this.setState({ id })\r\n        }\r\n\r\n    }\r\n    getDepartmentDetailed = async (id) => {\r\n        const { data: res } = await departmentDetailed({ id }).catch(err => err)\r\n        if (res.resCode !== 0) {\r\n            message.error(res.message)\r\n            return\r\n        }\r\n        this.setState({\r\n            config: {\r\n                ...this.state.config, setFieldValue: res.data\r\n            }\r\n        })\r\n    }\r\n    onAdd = async (query) => {\r\n        this.setButLoading(true)\r\n        const { data: res } = await departmentAdd(query).catch(err => err)\r\n        if (res.resCode !== 0) {\r\n            this.setButLoading(false)\r\n            message.error(res.message)\r\n            return\r\n        }\r\n        this.setButLoading(false)\r\n        message.success('添加成功')\r\n    }\r\n    onEdit = async (query) => {\r\n        this.setButLoading(true)\r\n        query.id = this.state.id\r\n        const { data: res } = await departmentEdit(query).catch(err => err)\r\n        if (res.resCode !== 0) {\r\n            this.setButLoading(false)\r\n            message.error(res.message)\r\n            return\r\n        }\r\n        this.setButLoading(false)\r\n        message.success('编辑成功')\r\n    }\r\n    onSubmit = async (e) => {\r\n        console.log(4444,e)\r\n        this.state.id ? this.onEdit(e) : this.onAdd(e)\r\n    }\r\n    setButLoading = (val) => {\r\n        this.setState({\r\n            butLoading: val\r\n        })\r\n    }\r\n    render () {\r\n        let { butLoading, config } = this.state\r\n        return (\r\n            <div>\r\n                <CustomForm config={config} onSubmit={this.onSubmit}></CustomForm>\r\n            </div >\r\n        );\r\n    }\r\n}\r\n\r\nexport default DepartmentAdd","C:\\Users\\Li\\Desktop\\react-admin\\src\\view\\position\\list.jsx",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\view\\position\\add.jsx",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\view\\control\\index.jsx",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\http\\api\\department.js",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\components\\CustomTable\\index.jsx",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\http\\api\\requestUrl.js",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\components\\CustomTable\\table.jsx",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\http\\api\\comm.js",[],"C:\\Users\\Li\\Desktop\\react-admin\\src\\components\\CustomForm\\index.jsx",["124"],"import React, { Component } from 'react';\r\nimport { Form, Button, Input, Radio, InputNumber, TextArea } from \"antd\"\r\n\r\n\r\nconst layout = {\r\n    labelCol: { span: 2 },\r\n    wrapperCol: { span: 10 },\r\n};\r\n\r\nclass CustomForm extends Component {\r\n    constructor(props) {\r\n        super(props)\r\n        this.form = React.createRef()\r\n        this.state = {\r\n            hintType: {\r\n                'Input': '请输入',\r\n                'TextArea': '请输入',\r\n                'InputNumber': '请输入',\r\n                'Radio': '请选择'\r\n            }\r\n        }\r\n    }\r\n    \r\n    // 监听props是否变化\r\n    componentDidUpdate(){\r\n       if(this.props.config.setFieldValue){\r\n           this.form.current.setFieldsValue(this.props.config.setFieldValue)\r\n       }\r\n   }\r\n    onFinish = (e) => {\r\n        this.props.onSubmit(e)\r\n    }\r\n\r\n    marked = (item) => {\r\n        return this.state.hintType[item.type] + item.label\r\n    }\r\n\r\n    rules = (item) => {\r\n        let rules = []\r\n        if (item.required) {\r\n            let message = this.marked(item)\r\n            rules.push({ required: true, message })\r\n        }\r\n        if (item.rules) {\r\n            rules = rules.concat(item.rules)\r\n        }\r\n        return rules\r\n    }\r\n\r\n    elemInput = (item) => {\r\n        return (\r\n            <Form.Item label={item.label} name={item.name} rules={this.rules(item)} key={item.name}>\r\n                <Input placeholder={item.placeholder || this.marked(item)} style={{ width: '100%' }} />\r\n            </Form.Item>\r\n        )\r\n    }\r\n\r\n    elemTextArea = (item) => {\r\n        return (\r\n            <Form.Item label={item.label} name={item.name} rules={this.rules(item)} key={item.name}>\r\n                <Input.TextArea placeholder={item.placeholder || this.marked(item)} style={{ width: '100%' }} />\r\n            </Form.Item>\r\n        )\r\n    }\r\n\r\n    elemInputNumber = (item) => {\r\n        return (\r\n            <Form.Item label={item.label} name={item.name} rules={this.rules(item)} key={item.name}>\r\n                <InputNumber min={item.min} max={item.max} placeholder={item.placeholder || this.marked(item)} style={{ width: '100%' }} />\r\n            </Form.Item>\r\n        )\r\n    }\r\n\r\n    elemRadio = (item) => {\r\n        return (\r\n            <Form.Item label={item.label} name={item.name} rules={this.rules(item)} key={item.name}>\r\n                <Radio.Group>\r\n                    {\r\n                        item.options && item.options.map(elem => {\r\n                            return <Radio value={elem.value} key={elem.value}>{elem.label}</Radio>\r\n                        })\r\n                    }\r\n                </Radio.Group>\r\n            </Form.Item>\r\n        )\r\n    }\r\n\r\n    initialize = () => {\r\n        let { formItem } = this.props.config\r\n        if (!formItem || (formItem && formItem.length === 0)) { return false }\r\n        let FormItemList = []\r\n        formItem.forEach(item => {\r\n            item.type === 'Input' && FormItemList.push(this.elemInput(item))\r\n            item.type === 'InputNumber' && FormItemList.push(this.elemInputNumber(item))\r\n            item.type === 'Radio' && FormItemList.push(this.elemRadio(item))\r\n            item.type === 'TextArea' && FormItemList.push(this.elemTextArea(item))\r\n        })\r\n        return FormItemList\r\n    }\r\n\r\n    render () {\r\n        let { initialValues } = this.props.config\r\n        return (\r\n            <div>\r\n                <Form {...layout} ref={this.form} initialValues={initialValues} onFinish={this.onFinish}>\r\n                    {this.initialize()}\r\n                    <Form.Item>\r\n                        <Button type=\"primary\" htmlType=\"submit\">保存</Button>\r\n                    </Form.Item>\r\n                </Form>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default CustomForm;","C:\\Users\\Li\\Desktop\\react-admin\\src\\components\\searchForm